{
  "expo": {
    "name": "TiffinTales",
    "slug": "tiffintales",
    "scheme": "tiffintales",
    "version": "1.0.0",
    "orientation": "portrait",
    
    "icon": "./assets/loginlogo.png",
    
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1033"
    },
    
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sudarshan812.tiffintales"
    },
    
    "android": {
      "package": "com.sudarshan812.tiffintales",
      
      "adaptiveIcon": {
        "foregroundImage": "./assets/loginlogo.png",
        "backgroundColor": "#1C1033"
      },
      
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TiffinTales to use your location to find nearby chefs.",
          "locationWhenInUsePermission": "Allow TiffinTales to use your location to find nearby chefs."
        }
      ]
    ],
    
    "extra": {
      "router": {},
      "eas": {
        "projectId": "cb2da514-5e91-4fbb-b2b8-5965814d4e73"
      }
    }
  }
}